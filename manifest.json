{
  "manifest_version": 3,
  "name": "Keynotion",
  "version": "0.1.0",
  "action": {
    "default_popup": "src/apps/popup/index.html"
  },
  "description": "Keynotion is an extension that provides useful features for explanation and presentation using Notion.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": ["src/content.tsx"],
      "matches": ["https://www.notion.so/*", "https://*.notion.site/*"]
    }
  ]
}